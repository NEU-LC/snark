none[0]="cv-cat --file ../data/square.png 'dilate' | wc -c"

rectangle[0]="cv-cat --file ../data/square.png 'dilate=rectangle,,,,'      | wc -c"
rectangle[1]="cv-cat --file ../data/square.png 'dilate=rectangle,7,9,,'    | wc -c"
rectangle[2]="cv-cat --file ../data/square.png 'dilate=rectangle,,9,,'     | wc -c"
rectangle[3]="cv-cat --file ../data/square.png 'dilate=rectangle,7,,,'     | wc -c"
rectangle[4]="cv-cat --file ../data/square.png 'dilate=rectangle,7,,-1,'   | wc -c"
rectangle[5]="cv-cat --file ../data/square.png 'dilate=rectangle,7,,2,3'   | wc -c"
rectangle[6]="cv-cat --file ../data/square.png 'dilate=rectangle,7,11,2,3' | wc -c"
rectangle[7]="cv-cat --file ../data/square.png 'dilate=rectangle,7,11,2,3,5' | wc -c"

square[0]="cv-cat --file ../data/square.png 'dilate=square,,'         | wc -c"
square[1]="cv-cat --file ../data/square.png 'dilate=square,7,2'       | wc -c"
square[2]="cv-cat --file ../data/square.png 'dilate=square,,-1'       | wc -c"
square[3]="cv-cat --file ../data/square.png 'dilate=square,,1'        | wc -c"
square[4]="cv-cat --file ../data/square.png 'dilate=square,7,'        | wc -c"
square[5]="cv-cat --file ../data/square.png 'dilate=square,7,2,3'     | wc -c"
square[6]="cv-cat --file ../data/square.png 'dilate=square,7,2,3,3'   | wc -c"

