test_cube[0]="points-make test-cube -n 3 --seed 0"

binary[0]="points-make test-cube -n 3 --seed 0 --binary | csv-from-bin $( points-make test-cube --output-format )"

test_grid[0]="points-make test-grid"
test_grid[1]="points-make test-grid --width 2 --spacing 1 --z-offset 0"
test_grid[2]="points-make test-grid --width 6 --spacing 3 --z-offset 0"
test_grid[3]="points-make test-grid --width 2 --spacing 1 --z-offset 1"
