binary/matching[0]="( echo 0,0,0,0 | csv-to-bin 4i | points-join-parallel <( { echo 1,0,0,0; } | csv-to-bin 4i )';fields=id,x,y,z;binary=4i' --fields id,x,y,z --radius 10 --binary 4i | csv-from-bin 8i; comma_status_ok || exit 1 )"
binary/matching[1]="( echo 0,0,0,0 | csv-to-bin 4i | points-join-parallel <( { echo 0,1,1,1; echo 1,0,0,0; } | csv-to-bin 4i )';fields=id,x,y,z;binary=4i' --fields id,x,y,z --radius 10 --binary 4i | csv-from-bin 8i; comma_status_ok || exit 1 )"
binary/matching[2]="( echo 0,0,0,0 | csv-to-bin 4i | points-join-parallel <( { echo 1,0,-1,-1,0,1,-1,0,0,1; } | csv-to-bin 10i )';fields=id,corners;binary=10i' --fields id,x,y,z --radius 10 --binary 4i | csv-from-bin 4i,10i,3d; comma_status_ok || exit 1 )"
binary/matching[3]="( echo 0,0,0,0 | csv-to-bin 4i | points-join-parallel <( { echo 0,1,-1,-1,1,1,-1,1,0,1; echo 1,0,-1,-1,0,1,-1,0,0,1; } | csv-to-bin 10i )';fields=id,corners;binary=10i' --fields id,x,y,z --radius 10 --binary 4i | csv-from-bin 4i,10i,3d; comma_status_ok || exit 1 )"

binary/not_matching[0]="( echo 0,0,0,0 | csv-to-bin 4i | points-join-parallel <( { echo 0,0,0,0; } | csv-to-bin 4i )';fields=id,x,y,z;binary=4i' --fields id,x,y,z --radius 10 --not-matching-id --binary 4i | csv-from-bin 4i,4i; comma_status_ok || exit 1 )"
binary/not_matching[1]="( echo 0,0,0,0 | csv-to-bin 4i | points-join-parallel <( { echo 0,0,0,0; echo 1,1,1,1; } | csv-to-bin 4i )';fields=id,x,y,z;binary=4i' --fields id,x,y,z --radius 10 --not-matching-id --binary 4i | csv-from-bin 4i,4i; comma_status_ok || exit 1 )"
binary/not_matching[2]="( echo 0,0,0,0 | csv-to-bin 4i | points-join-parallel <( { echo 0,1,-1,-1,1,1,-1,1,0,1; } | csv-to-bin 10i )';fields=id,corners;binary=10i' --fields id,x,y,z --radius 10 --not-matching-id --binary 4i | csv-from-bin 4i,10i,3d; comma_status_ok || exit 1 )"
binary/not_matching[3]="( echo 0,0,0,0 | csv-to-bin 4i | points-join-parallel <( { echo 0,1,-1,-1,1,1,-1,1,0,1; echo 1,0,-1,-1,0,1,-1,0,0,1; } | csv-to-bin 10i )';fields=id,corners;binary=10i' --fields id,x,y,z --radius 10 --not-matching-id --binary 4i | csv-from-bin 4i,10i,3d; comma_status_ok || exit 1 )"
