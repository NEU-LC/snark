strict/failure[0]="echo 0,0,0 | points-join <( ) --radius 10 --strict"
strict/failure[1]="echo 0,0,0 | points-join <( ) --radius 10 --strict --matching"
strict/failure[2]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | points-join <( echo 0,0,0 ) --radius 1 --strict"
strict/failure[3]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | points-join <( echo 0,0,0 ) --radius 1 --strict --matching"
strict/failure[4]="echo 0,0,0 | points-join <( echo 0,0,0 ) --min-radius 0.5 --radius 1 --matching --strict"
strict/failure[5]="( echo 0,0,0,0; echo 0,0,0,1 ) | points-join <( echo 0,0,0,1 )';fields=x,y,z,block' --fields x,y,z,block --radius 1 --blocks-ordered --strict"
strict/failure[6]="( echo 0,0,0,0; echo 0,0,0,1 ) | points-join <( echo 0,0,0,0 )';fields=x,y,z,block' --fields x,y,z,block --radius 1 --blocks-ordered --strict"
strict/failure[7]="( echo 0,0,0,0; echo 0,0,0,1 ) | points-join <( echo 0,0,0,1 )';fields=x,y,z,block' --fields x,y,z,block --radius 1 --blocks-ordered --strict --matching"
strict/failure[8]="( echo 0,0,0,0; echo 0,0,0,1 ) | points-join <( echo 0,0,0,0 )';fields=x,y,z,block' --fields x,y,z,block --radius 1 --blocks-ordered --strict --matching"
strict/failure[9]="( echo 0,0,0,0; echo 0,0,0,1; echo 0,0,0,2 ) | points-join <( echo 0,0,0,0; echo 0,0,0,1 )';fields=x,y,z,block' --fields x,y,z,block --radius 1 --strict"
strict/failure[10]="( echo 0,0,0,0; echo 0,0,0,1; echo 0,0,0,2 ) | points-join <( echo 0,0,0,0; echo 0,0,0,1 )';fields=x,y,z,block' --fields x,y,z,block --radius 1 --strict --matching"
strict/failure[11]="echo | points-join <( ) --radius 10 --strict --not-matching"

strict/success[0]="echo | points-join <( ) --radius 10 --strict"
strict/success[1]="echo | points-join <( ) --radius 10 --strict --matching"
strict/success[2]="echo 0,0,0 | points-join <( echo 0,0,0 ) --radius 1 --strict"
strict/success[3]="echo 0,0,0 | points-join <( echo 0,0,0 ) --radius 1 --matching --strict"
strict/success[4]="echo 0,0,0 | points-join <( echo 0,0,0 ) --min-radius 0 --radius 1 --matching --strict"
strict/success[5]="( echo 0,0,0,0; echo 0,0,0,1 ) | points-join <( echo 0,0,0,0; echo 0,0,0,1 )';fields=x,y,z,block' --fields x,y,z,block --radius 1 --strict"
