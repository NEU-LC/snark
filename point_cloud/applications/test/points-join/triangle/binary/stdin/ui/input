triangles/binary/ui[0]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | csv-to-bin 3ui | points-join-parallel <( { echo 0,0,0,1,1,1,1,1,0; } | csv-to-bin 9f )';fields=corners;binary=9f' --radius 2 --binary 3ui | csv-from-bin 3ui,9f,3d"
triangles/binary/ui[1]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | csv-to-bin 3ui | points-join-parallel <( { echo 0,0,0,1,1,1,1,1,0; } | csv-to-bin 9f )';fields=corners;binary=9f' --radius 2 --binary 3ui --matching | csv-from-bin 3ui"
triangles/binary/ui[2]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | csv-to-bin 3ui | points-join-parallel <( { echo 0,0,0,1,1,1,1,1,0; } | csv-to-bin 9f )';fields=corners;binary=9f' --radius 2 --binary 3ui --not-matching | csv-from-bin 3ui"
triangles/binary/ui[3]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | csv-to-bin 3ui | points-join-parallel <( { echo 0,0,0,1,1,1,1,1,0; echo 1,1,1,2,2,2,2,1,2; } | csv-to-bin 9f )';fields=corners;binary=9f' --radius 2 --binary 3ui --size 1 | csv-from-bin 3ui,9f,3d"
triangles/binary/ui[4]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | csv-to-bin 3ui | points-join-parallel <( { echo 0,0,0,1,1,1,1,1,0; echo 1,1,1,2,2,2,2,1,2; } | csv-to-bin 9f )';fields=corners;binary=9f' --radius 2 --binary 3ui --size 2 | csv-from-bin 3ui,9f,3d"
triangles/binary/ui[5]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | csv-to-bin 3ui | points-join-parallel <( { echo 0,0,0,1,1,1,1,1,0; echo 1,1,1,2,2,2,2,1,2; echo 0,0,0,0.5,0.5,0.5,0.5,0,0; } | csv-to-bin 9f )';fields=corners;binary=9f' --radius 2 --binary 3ui --all | csv-from-bin 3ui,9f"
