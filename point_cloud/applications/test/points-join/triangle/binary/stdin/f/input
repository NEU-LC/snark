triangles/binary/f[0]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | csv-to-bin 3f | points-join <( { echo 0,0,0,1,1,1,1,1,0; } | csv-to-bin 9f )';fields=corners;binary=9f' --radius 2 --binary 3f | csv-from-bin 3f,9f,3d"
triangles/binary/f[1]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | csv-to-bin 3f | points-join <( { echo 0,0,0,1,1,1,1,1,0; } | csv-to-bin 9f )';fields=corners;binary=9f' --radius 2 --matching --binary 3f | csv-from-bin 3f"
triangles/binary/f[2]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | csv-to-bin 3f | points-join <( { echo 0,0,0,1,1,1,1,1,0; } | csv-to-bin 9f )';fields=corners;binary=9f' --radius 2 --not-matching --binary 3f | csv-from-bin 3f"
triangles/binary/f[3]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | csv-to-bin 3f | points-join <( { echo 0,0,0,1,1,1,1,1,0; echo 1,1,1,2,2,2,2,1,2; } | csv-to-bin 9f )';fields=corners;binary=9f' --radius 2 --size 1 --binary 3f | csv-from-bin 3f,9f,3d"
triangles/binary/f[4]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | csv-to-bin 3f | points-join <( { echo 0,0,0,1,1,1,1,1,0; echo 1,1,1,2,2,2,2,1,2; } | csv-to-bin 9f )';fields=corners;binary=9f' --radius 2 --size 2 --binary 3f | csv-from-bin 3f,9f,3d"
triangles/binary/f[5]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | csv-to-bin 3f | points-join <( { echo 0,0,0,1,1,1,1,1,0; echo 1,1,1,2,2,2,2,1,2; echo 0,0,0,0.5,0.5,0.5,0.5,0,0; } | csv-to-bin 9f )';fields=corners;binary=9f' --radius 2 --all --binary 3f | csv-from-bin 3f,9f"
