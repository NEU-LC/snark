triangles/binary/i[0]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | csv-to-bin 3i | points-join <( { echo 0,0,0,1,1,1,1,1,0; } | csv-to-bin 9f )';fields=corners;binary=9f' --radius 2 --binary 3i | csv-from-bin 3i,9f,3d"
triangles/binary/i[1]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | csv-to-bin 3i | points-join <( { echo 0,0,0,1,1,1,1,1,0; } | csv-to-bin 9f )';fields=corners;binary=9f' --radius 2 --matching --binary 3i | csv-from-bin 3i"
triangles/binary/i[2]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | csv-to-bin 3i | points-join <( { echo 0,0,0,1,1,1,1,1,0; } | csv-to-bin 9f )';fields=corners;binary=9f' --radius 2 --not-matching --binary 3i | csv-from-bin 3i"
triangles/binary/i[3]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | csv-to-bin 3i | points-join <( { echo 0,0,0,1,1,1,1,1,0; echo 1,1,1,2,2,2,2,1,2; } | csv-to-bin 9f )';fields=corners;binary=9f' --radius 2 --size 1 --binary 3i | csv-from-bin 3i,9f,3d"
triangles/binary/i[4]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | csv-to-bin 3i | points-join <( { echo 0,0,0,1,1,1,1,1,0; echo 1,1,1,2,2,2,2,1,2; } | csv-to-bin 9f )';fields=corners;binary=9f' --radius 2 --size 2 --binary 3i | csv-from-bin 3i,9f,3d"
triangles/binary/i[5]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | csv-to-bin 3i | points-join <( { echo 0,0,0,1,1,1,1,1,0; echo 1,1,1,2,2,2,2,1,2; echo 0,0,0,0.5,0.5,0.5,0.5,0,0; } | csv-to-bin 9f )';fields=corners;binary=9f' --radius 2 --all --binary 3i | csv-from-bin 3i,9f"
