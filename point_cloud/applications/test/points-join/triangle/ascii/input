triangles/ascii[0]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | points-join <( { echo 0,0,0,1,1,1,1,1,0; } )';fields=corners' --radius 2"
triangles/ascii[1]="( echo 0,0,0; echo 1,1,1; echo 2,2,2 ) | points-join <( { echo 0,0,0,1,1,1,1,1,0; } )';fields=corners' --radius 2 --matching"
