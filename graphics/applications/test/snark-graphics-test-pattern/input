cube_points[0]="snark-graphics-test-pattern cube-points -n 3 --seed 0"

binary[0]="snark-graphics-test-pattern cube-points -n 3 --seed 0 --binary | csv-from-bin $( snark-graphics-test-pattern cube-points --output-format )"

grid[0]="snark-graphics-test-pattern grid"
grid[1]="snark-graphics-test-pattern grid --width 2 --spacing 1 --z-offset 0"
grid[2]="snark-graphics-test-pattern grid --width 6 --spacing 3 --z-offset 0"
grid[3]="snark-graphics-test-pattern grid --width 2 --spacing 1 --z-offset 1"
